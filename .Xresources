
! Xft resources
Xft.antialias:true
Xft.dpi:96
Xft.hinting:true
! hintslight | hintsmedium | hintsfull
Xft.hintstyle:hintslight
! rgba subpixel hinting (for LCDs)
Xft.rgba:none

! Xterm resources

! Xft font name style
XTerm*faceName:Liberation Mono
XTerm*faceSize:12
xterm*locale: true
xterm*termName: rxvt-256color
xterm*inheritPixmap: False
xterm*imLocale: en_US.UTF-8
xterm*scrollBar: false
xterm*saveLines: 5000
xterm*urlLauncher: luakit
xterm*perl-ext-common: default,matcher
xterm*matcher.button: 2
xterm*matcher.pattern.1: \\bwww\\.[\\w-]\\.[\\w./?&@#-]*[\\w/-]
xterm*cursorBlink: true
xterm*geometry: 120x25
xterm*fading: 0%
xterm*shading: 75

*foreground: #EFEFDE
*background: #262626

*color0: #acd3d5
*color1: #69a8a6
*color2: #253854
*color3: #244f68
*color4: #85d4ca
*color5: #2b5568
*color6: #286785
*color7: #549298
*color8: #388bad
*color9: #49b4d8
*color10: #4e638d
*color11: #205f7c
*color12: #65c4d1
*color13: #28a0ca
*color14: #2379a1
*color15: #51a0c1

